import java.util.*;
class Areaoftriangle
{
  public static void main(String arg[])
    {
      Scanner sc=new Scanner(System.in);
      double b,h,Area;
      System.out.println("enter b");
      b=sc.nextDouble();
      System.out.println("enter h");
      h=sc.nextDouble();
      Area=0.5*b*h;
      System.out.println("Area:"+Area);
    } 
}
